  TRACFONE_USA_STATUS_PRO         String?              @db.Text
  LG_INFO                         String?              @db.Text
  ZTE_INFO                        String?              @db.Text
  ASUS_INFO                       String?              @db.Text
  OPPO_INFO                       String?              @db.Text
  ACER_INFO                       String?              @db.Text
