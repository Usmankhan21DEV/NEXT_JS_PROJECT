model order_history {
  SEQ_NUM                  Int                       @id @default(autoincrement())
  USER_SEQ_NUM             Int?
  ORDER_ID                 String?                   @db.VarChar(45)
  INFO_SEQ_NUM             Int?
  IMEI                     String?                   @db.VarChar(45)
  SERVICE_SEQ_NUM          Int?
  SERVICE_COST             Decimal?                  @db.Decimal(10, 2)
  SEARCH_DATE              DateTime?                 @default(now()) @db.DateTime(0)
  STATUS                   String?                   @db.VarChar(20)
  SERVICE_NAME             String?                   @db.VarChar(45)
  USER_SERVICE_NAME        String?                   @db.VarChar(45)
  COMPLETED_DATE           DateTime?                 @db.DateTime(0)
  REQUESTED_EMAIL          String?                   @db.VarChar(45)
  user_info                user_info?                @relation(fields: [USER_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "ORDER_HISTORY_ibfk_1")
  info_delay               info_delay?               @relation(fields: [INFO_SEQ_NUM], references: [ID_SEQ], onDelete: NoAction, onUpdate: NoAction, map: "ORDER_HISTORY_ibfk_2")
  package_service_junction package_service_junction? @relation(fields: [SERVICE_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "ORDER_HISTORY_ibfk_3")

  @@index([INFO_SEQ_NUM], map: "INFO_SEQ_NUM")
  @@index([SERVICE_SEQ_NUM], map: "ORDER_HISTORY_ibfk_3")
  @@index([USER_SEQ_NUM], map: "USER_SEQ_NUM")
}

model package_service_junction {
  SEQ_NUM            Int                  @id @default(autoincrement())
  SERVICE_SEQ_NUM    Int
  PACKAGE_SEQ_NUM    Int
  COST               Decimal              @default(0.00) @db.Decimal(10, 2)
  CREATE_DATE        DateTime?            @default(now()) @db.DateTime(0)
  imei_user_junction imei_user_junction[]
  order_history      order_history[]
  packages           packages             @relation(fields: [PACKAGE_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "fk_package")
  services           services             @relation(fields: [SERVICE_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "fk_service")
  refund             refund[]

  @@index([PACKAGE_SEQ_NUM], map: "fk_package")
  @@index([SERVICE_SEQ_NUM], map: "fk_service")
}

model packages {
  SEQ_NUM                  Int                        @id @unique(map: "SEQ_NUM") @default(autoincrement())
  NAME                     String                     @db.VarChar(100)
  CREATE_DATE              DateTime?                  @default(now()) @db.DateTime(0)
  package_service_junction package_service_junction[]
  payment_detail           payment_detail[]
  refund                   refund[]
}

model payment_detail {
  SEQ_NUM         Int        @id @default(autoincrement())
  TRANS_ID        String?    @db.VarChar(255)
  PAYMENT_METHOD  String?    @db.VarChar(255)
  STATUS          String?    @db.VarChar(1000)
  USER_SEQ_NUM    Int?
  BUYER_EMAIL     String?    @db.VarChar(255)
  FIRST_NAME      String?    @db.VarChar(255)
  LAST_NAME       String?    @db.VarChar(255)
  BUYER_ID        String?    @db.VarChar(255)
  AMOUNT          Decimal    @default(0.00) @db.Decimal(10, 2)
  PACKAGE_SEQ_NUM Int?
  TAX             String?    @db.VarChar(255)
  CREATE_DATETIME DateTime?  @db.DateTime(0)
  UPDATE_DATETIME DateTime?  @db.DateTime(0)
  packages        packages?  @relation(fields: [PACKAGE_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "fk_package_balance")
  user_info       user_info? @relation(fields: [USER_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "payment_detail_ibfk_1")

  @@index([USER_SEQ_NUM], map: "USER_SEQ_NUM")
  @@index([PACKAGE_SEQ_NUM], map: "fk_package_balance")
}

model payment_types {
  SEQ_NUM           Int      @id @unique(map: "PAYMENT_TYPES_PK") @default(autoincrement())
  PAYMENT_TYPE_NAME String   @db.VarChar(50)
  ENTRY_DATE        DateTime @default(now()) @db.DateTime(0)
}

model refund {
  SEQ_NUM                  Int                       @id @default(autoincrement())
  AMOUNT                   Decimal                   @db.Decimal(10, 2)
  SERVICE_NO               Int?
  PACKAGE_SEQ_NUM          Int?
  BALANCE_SEQ_NUM          Int?
  USER_SEQ_NUM             Int
  REASON                   String?                   @db.Text
  CREATED_DATE             DateTime?                 @default(now()) @db.DateTime(0)
  user_info                user_info                 @relation(fields: [USER_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "REFUND_FOREIGN_KEY_1")
  package_service_junction package_service_junction? @relation(fields: [SERVICE_NO], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "REFUND_FOREIGN_KEY_2")
  packages                 packages?                 @relation(fields: [PACKAGE_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "REFUND_FOREIGN_KEY_3")
  user_balance             user_balance?             @relation(fields: [BALANCE_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "REFUND_FOREIGN_KEY_4")

  @@index([USER_SEQ_NUM], map: "REFUND_FOREIGN_KEY_1_IDX")
  @@index([SERVICE_NO], map: "REFUND_FOREIGN_KEY_2_IDX")
  @@index([PACKAGE_SEQ_NUM], map: "REFUND_FOREIGN_KEY_2_IDX1")
  @@index([BALANCE_SEQ_NUM], map: "REFUND_FOREIGN_KEY_4_IDX")
}


model services {
  SEQ_NUM                  Int                        @id @unique(map: "SEQ_NUM") @default(autoincrement())
  NAME                     String                     @db.VarChar(100)
  CREATE_DATE              DateTime?                  @default(now()) @db.DateTime(0)
  APIKEY                   Int?                       @unique(map: "ApiKey_UNIQUE")
  SERVICE                  String?                    @db.VarChar(255)
  TYPE                     String?                    @db.VarChar(45)
  DELAY                    String?                    @default("NO") @db.VarChar(45)
  package_service_junction package_service_junction[]
}

model user_balance {
  SEQ_NUM         Int        @id @default(autoincrement())
  USER_SEQ_NUM    Int?
  BALANCE         Decimal?   @default(0.00) @db.Decimal(10, 2)
  PACKAGE_SEQ_NUM Int?
  CREATED_DATE    DateTime?  @default(now()) @db.DateTime(0)
  refund          refund[]
  user_info       user_info? @relation(fields: [USER_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "user_balance_ibfk_1")

  @@index([USER_SEQ_NUM], map: "USER_SEQ_NUM")
}

model user_comment {
  SEQ_NUM      Int       @id @default(autoincrement())
  NAME         String?   @db.VarChar(255)
  EMAIL        String?   @db.VarChar(255)
  COMMENT      String?   @db.VarChar(1000)
  CREATED_DATE DateTime? @db.DateTime(0)
}

model user_info {
  SEQ_NUM            Int                  @id @default(autoincrement())
  FIRST_NAME         String               @db.VarChar(255)
  LAST_NAME          String               @db.VarChar(255)
  EMAIL              String               @unique(map: "EMAIL") @db.VarChar(255)
  CONTACT_NUMBER     String               @unique(map: "CONTACT_NUMBER") @db.VarChar(255)
  ADDRESS_1          String               @db.VarChar(255)
  ADDRESS_2          String               @db.VarChar(255)
  CITY               String               @db.VarChar(255)
  STATE              String               @db.VarChar(255)
  COUNTRY            String               @db.VarChar(255)
  ZIP_POSTAL_CODE    String               @db.VarChar(255)
  PASSWORD           String               @db.VarChar(255)
  TOKEN              String?              @db.VarChar(255)
  VARIFIED           Boolean?
  TRAIL_PERIOD       Boolean?
  IS_ACTIVE          Boolean?
  IS_LOGIN           Boolean?
  USER_IP            String?              @db.VarChar(255)
  USER_BROWSER       String?              @db.VarChar(255)
  USER_HOST_NAME     String?              @db.VarChar(255)
  DEVICE_TYPE        String?              @db.VarChar(255)
  ROLES              Int?                 @default(1)
  DEFAULT_EMAIL      Int?                 @default(0)
  imei_user_junction imei_user_junction[]
  order_history      order_history[]
  payment_detail     payment_detail[]
  refund             refund[]
  user_balance       user_balance[]
  user_roles         user_roles?          @relation(fields: [ROLES], references: [ROLE_ID], onDelete: NoAction, onUpdate: NoAction, map: "fk_roles")

  @@index([ROLES], map: "fk_roles")
}

model user_roles {
  ROLE_ID     Int         @id @default(autoincrement())
  ROLE_NAME   String?     @db.VarChar(255)
  ROLE_RIGHTS String?     @db.VarChar(255)
  CREATE_DATE DateTime?   @default(now()) @db.DateTime(0)
  user_info   user_info[]
}
