generator client {
  provider         = "prisma-client-js"
  premodelFeatures = ["models"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}











model imei_user_junction {
  SEQ_NUM                  Int                       @id @default(autoincrement())
  USER_SEQ_NUM             Int?
  INFO_SEQ_NUM             Int?
  INFO_D_SEQ_NUM           Int?
  SERVICE_NO               Int?
  SERVICE_FEE              Decimal?                  @default(0.00) @db.Decimal(10, 2)
  SEARCH_DATE              DateTime?                 @db.DateTime(0)
  user_info                user_info?                @relation(fields: [USER_SEQ_NUM], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "imei_user_junction_ibfk_1")
  info                     info?                     @relation(fields: [INFO_SEQ_NUM], references: [ID_SEQ], onDelete: NoAction, onUpdate: NoAction, map: "imei_user_junction_ibfk_2")
  info_delay               info_delay?               @relation(fields: [INFO_D_SEQ_NUM], references: [ID_SEQ], onDelete: NoAction, onUpdate: NoAction, map: "imei_user_junction_ibfk_3")
  package_service_junction package_service_junction? @relation(fields: [SERVICE_NO], references: [SEQ_NUM], onDelete: NoAction, onUpdate: NoAction, map: "imei_user_junction_ibfk_4")

  @@index([USER_SEQ_NUM], map: "imei_user_junction_ibfk_1")
  @@index([INFO_SEQ_NUM], map: "imei_user_junction_ibfk_2")
  @@index([INFO_D_SEQ_NUM], map: "imei_user_junction_ibfk_2_idx")
  @@index([SERVICE_NO], map: "imei_user_junction_ibfk_4_idx")
}

